{
  "name": "pccomponentes/messenger-bundle",
  "description": "Symfony Messenger Bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Zoilo Mora",
      "email": "zoilo.mora@pccomponentes.com"
    },
    {
      "name": "Aar√≥n Bernabeu",
      "email": "aaron.bernabeu@pccomponentes.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PcComponentes\\SymfonyMessengerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PcComponentes\\SymfonyMessengerBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "ext-amqp": "*",
    "php": "^8.0",
    "pccomponentes/ddd-logging": "^2.5",
    "pccomponentes/ddd": "^4.0",
    "beberlei/assert": "^3.3",
    "ramsey/uuid": "^4.5",
    "symfony/framework-bundle": "^6.0",
    "symfony/amqp-messenger": "^6.0"
  },
  "require-dev": {
    "pccomponentes/coding-standard": "^2.0",
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  }
}
